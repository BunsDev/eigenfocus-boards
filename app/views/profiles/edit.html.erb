<div class="mx-auto w-full md:w-2/3">
  <div class="text-3xl font-bold mb-5 text-body-500">
    <h2 class="font-semibold text-body-900">
      <%= "#{t('.title')}" %>
    </h2>
  </div>
  <div class="card p-5 rounded-lg">
    <% unless current_user.is_profile_complete? %>
      <%= render partial: "layouts/alert", locals: { type: 'alert', message: t(".setup_missing") } %>
    <% end %>

    <%= render(partial: 'form') %>
  </div>
</div>